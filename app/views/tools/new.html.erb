<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
<div class="container">

  <%= form_for(@tool) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :price %>
    <%= f.text_field :price %>
    <%= f.label :quantity %>
    <%= f.text_field :quantity %>
    <button type="submit" class="btn btn-danger active"><%= f.submit %></button>
  <% end %>
</div>
<img src="http://www.harborfreight.com/media/catalog/product/cache/1/image/9df78eab33525d08d6e5fb8d27136e95/i/m/image_23816.jpg" class="img-responsive" alt="Responsive image">
